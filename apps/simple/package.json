    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "~0.19.9",
    "tamagui": "1.89.12"
  },
  "devDependencies": {
    "@tamagui/vite-plugin": "1.89.12",
    "zx": "^7.2.3",
    "wait-port": "^0.3.0",
    "@playwright/test": "^1.40.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "async-retry": "1.3.1",
    "vite": "^5.0.12"
